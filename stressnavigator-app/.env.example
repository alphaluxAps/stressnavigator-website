# AWS Deployment Configuration
# Copy this file to .env and fill in your values

# S3 Bucket name where the website will be hosted
S3_BUCKET=stressnavigator.dk

# CloudFront Distribution ID for cache invalidation
CLOUDFRONT_DISTRIBUTION_ID=E2JEO1H1I7ELL6

# AWS CLI Profile to use (optional, defaults to 'default')
# This should correspond to a profile in your ~/.aws/credentials file
AWS_PROFILE=AlphaLux

# Example values:
# S3_BUCKET=arn:aws:s3:::stressnavigator.dk
# CLOUDFRONT_DISTRIBUTION_ID=E1234567890ABC
# AWS_PROFILE=my-aws-profile
